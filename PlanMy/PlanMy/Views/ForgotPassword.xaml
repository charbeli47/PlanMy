        <StackLayout>
            <Label Text="Username" FontSize="16" TextColor="Black" x:Name="UsernameLabel" Margin="20,0,20,0"/>
            <Entry TextColor="Black" Placeholder="Type your username here" x:Name="UsernameEntry" Margin="20,0,20,0"/>
           
        </StackLayout>
    </ContentPage.Content>
</ContentPage>